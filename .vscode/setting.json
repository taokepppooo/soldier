{
  "cSpell.words": ["Element Plus", "element-plus"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "stylelint.validate": ["css", "less", "postcss", "scss", "vue", "sass"],
  "stylelint.disableErrorMessage": true,
  "stylelint.enable": true,
  "stylelint.useLocal": true,
  "stylelint.configFile": "stylelint.config.js",
  "stylelint.packageManager": "pnpm",
  "npm.packageManager": "pnpm",
  "eslint.packageManager": "pnpm",
  "eslint.probe": ["javascript", "typescript", "html", "vue", "markdown"],
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue",
    "markdown"
  ],
  "vite.devCommand": "pnpm run dev -- --",
  "markdownlint.customRules": [".markdownlint.json"],
  "vetur.format.enable": false,
  "vetur.validation.style": false,
  "vetur.validation.template": false,
  "unocss.root": "packages/client"
}
